    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@vitejs/plugin-react": "^3.0.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.1.9",
