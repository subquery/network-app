    "@types/js-yaml": "^4.0.3",
    "@types/lru-cache": "^5.1.1",
    "@types/moment-duration-format": "^2.2.3",
    "@types/ramda": "^0.27.50",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-modal": "^3.13.1",
    "@types/react-router": "^5.1.16",
